Maybe[T] (
    | 0 = Some, T
    | 1 = None
)

Employee {
    name str,
    age ux,
    salary f32,
}

add(a f32, b f32) f32

add(a=1, b=2)

Linked List:

Node {
    content = u32,
    next | { Some, node -> Node }
         | None
}

Node { content = 0, next = (Some, -)}->
Node { content = 1, next = (Some, -)}->
Node { content = 1, next = None }

Boolean : Copy ( true | false )

Slice : Copy {
    len ux,
    ptr ->(len * [])
}
Slice[a ->(len * []), len ux] = Slice { len, }
